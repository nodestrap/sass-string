{
  "name": "@nodestrap/sass-string",
  "version": "2.0.7",
  "description": "A sass utility for manipulating strings.",
  "dependencies": {},
  "devDependencies": {
    "tilde-sass": "^1.0.2"
  },
  "scripts": {
    "test": "tilde-sass --file test/*.scss --outFile test/ --mergeSelectors",
    "test-watch": "tilde-sass --file test/*.scss --outFile test/ --mergeSelectors --watch",
    "deploy": "tilde-sass --file src/*.scss --outFile dist/ --mergeSelectors --outputStyle compressed",
    "deploy-watch": "tilde-sass --file src/*.scss --outFile dist/ --mergeSelectors --outputStyle compressed --watch"
  },
  "cssnano": {
    "preset": [
      "default",
      {
        "calc": false
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodestrap/sass-string.git"
  },
  "keywords": [
    "sass",
    "string",
    "manipulation",
    "utility"
  ],
  "author": "nodestrap, heymarco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nodestrap/sass-string/issues"
  },
  "homepage": "https://github.com/nodestrap/sass-string#readme"
}
